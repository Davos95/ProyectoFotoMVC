
@{
    ViewBag.Title = "menu";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section events {
    <script>
        $(document).ready(function () {
            $('.modal').modal();
            $("#btnSesion").click(function () {
                $("#modalSesion").modal('open');
            });
            $("#btnParTar").click(function () {
                $("#modalParTar").modal('open');
            });
            $("#btnComision").click(function () {
                $("#modalComision").modal('open');
            });
        });
    </script>
}
<div class="row">
    <div class="col s12">
        <div class="row"></div>
        <div class="row">
            <div class="col s12 m4 l3 xl2 offset-m3 offset-l3 offset-xl3 ">
                <div class="card">
                    <div class="card-image">
                        <div style=" margin:0 auto;"> <i class="material-icons center" style="font-size:170px;">insert_photo</i></div>
                    </div>
                    <div class="card-content">
                        <h5>@Html.ActionLink("Subir Fotos", "UploadImages","Images")</h5>
                    </div>
                </div>
                
            </div>
            <div class="col s12 m4 l3 xl2 offset-m3 offset-l2 offset-xl1">
                <div class="card">
                    <div class="card-image">
                        <i class="material-icons" style="font-size:170px; margin:0 auto;">photo_camera</i>
                    </div>
                    <div class="card-content">
                        <h5>@Html.ActionLink("Sesiones", "Sesion", "Sesion")</h5>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col s12 m4 l3 xl2  offset-m3 offset-l3 offset-xl3 btnMenu">
                <div class="card">
                    <div class="card-image">
                        <i class="material-icons" style="font-size:170px; margin:0 auto;">portrait</i>
                    </div>
                    <div class="card-content">
                        <a class="center-align" id="btnParTar"><h5>Participantes y Trabajos</h5></a>
                    </div>
                </div>
                
            </div>
            <div class="col s12 m4 l3 xl2  offset-m3 offset-l2 offset-xl1 btnMenu">
                <div class="card">
                    <div class="card-image">
                        <i class="material-icons" style="font-size:170px; margin:0 auto;">monetization_on</i>
                    </div>
                    <div class="card-content">
                        <a class="center-align" id="btnComision"><h5>Comisiones</h5></a>
                    </div>
                </div>
                
            </div>
        </div>
    </div>
</div>

<div id="modalSesion" class="modal">
    <div class="modal-content">
        
    </div>
</div>

<div id="modalParTar" class="modal">
    <div class="modal-content">
        <div class="row">
            <div class="col s4">
                @Html.ActionLink("Participantes", "Partners", "PartnerWork")
            </div>
            <div class="col s4 offset-s2">
                @Html.ActionLink("Trabajos", "Works", "PartnerWork")
            </div>
        </div>
    </div>
</div>

<div id="modalComision" class="modal">
    <div class="modal-content">
        <div class="row">
            <div class="col s4">
                @Html.ActionLink("Comisiones", "Comision", "Comision")
            </div>
        </div>
    </div>
</div>